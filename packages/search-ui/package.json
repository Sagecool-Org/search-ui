{
  "name": "@elastic/search-ui",
  "version": "1.22.2",
  "description": "A Headless Search UI library",
  "license": "Apache-2.0",
  "main": "lib/cjs/index.js",
  "module": "lib/esm/index.js",
  "types": "lib/cjs/index.d.ms",
  "sideEffects": false,
  "homepage": "https://docs.elastic.co/search-ui",
  "directories": {
    "lib": "lib",
    "test": "__tests__"
  },
  "files": [
    "lib"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/elastic/search-ui.git",
    "directory": "packages/search-ui"
  },
  "scripts": {
    "test-ci": "jest --runInBand",
    "test": "jest",
    "build": "tsup --config tsup.config.ts -- --tsconfig tsconfig.json",
    "watch": "yarn run build --watch",
    "prepare": "yarn run build"
  },
  "bugs": {
    "url": "https://github.com/elastic/search-ui/issues"
  },
  "devDependencies": {
    "@types/history": "^4.7.11",
    "@types/qs": "^6.9.7",
    "typescript": "^4.9.3"
  },
  "dependencies": {
    "date-fns": "^1.30.1",
    "deep-equal": "^1.0.1",
    "history": "^4.9.0",
    "qs": "^6.9.7"
  },
  "exports": {
    ".": {
      "types": "./lib/cjs/index.d.ts",
      "import": "./lib/esm/index.js",
      "require": "./lib/cjs/index.js"
    },
    "./package": "./package.json",
    "./package.json": "./package.json"
  }
